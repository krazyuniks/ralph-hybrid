key_path=\"$2\"\n    41→\n    42→    if [[ ! -f \"$file\" ]]; then\n    43→        return 0\n    44→    fi\n    45→\n    46→    # Count the number of dots to determine nesting level\n    47→    local dot_count=\"${key_path//[^.]/}\"\n    48→    local nesting_level=${#dot_count}\n    49→\n    50→    if [[ $nesting_level -eq 0 ]]; then\n    51→        # Simple top-level key (no dot in key_path)\n    52→        while IFS= read -r line || [[ -n \"$line\" ]]; do\n    53→            # Pattern: ^${key_path}:[[:space:]]*(.*)$\n    54→            # Matches: Top-level YAML key-value pair (no indentation)\n    55→            if [[ \"$line\" =~ ^${key_path}:[[:space:]]*(.*)$ ]]; then\n    56→                local result=\"${BASH_REMATCH[1]}\"\n    57→                # Remove surrounding quotes if present\n    58→                result=\"${result#\\\"}\"\n    59→                result=\"${result%\\\"}\"\n    60→                result=\"${result#\\'}\"\n    61→                result=\"${result%\\'}\"\n    62→                echo \"$result\"\n    63→                return 0\n    64→            fi\n    65→        done < \"$file\"\n    66→    elif [[ $nesting_level -eq 1 ]]; then\n    67→        # Two-level nesting (e.g., \"defaults.max_iterations\")\n    68→        local section=\"${key_path%%.*}\"\n    69→        local key=\"${key_path#*.}\"\n    70→\n    71→        local in_section=0\n    72→\n    73→        while IFS= read -r line || [[ -n \"$line\" ]]; do\n    74→            # Check if we're entering the target section\n    75→            if [[ \"$line\" =~ ^${section}:$ ]] || [[ \"$line\" =~ ^${section}:[[:space:]]*$ ]]; then\n    76→                in_section=1\n    77→                continue\n    78→            fi\n    79→\n    80→            # Check if we've exited the section (new top-level key)\n    81→            if [[ $in_section -eq 1 ]] && [[ \"$line\" =~ ^[a-zA-Z_] ]] && [[ ! \"$line\" =~ ^[[:space:]] ]]; then\n    82→                in_section=0\n    83→           