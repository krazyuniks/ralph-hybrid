==============\n\n@test \"_verify_extract_verdict extracts VERIFIED\" {\n    local temp_file\n    temp_file=$(mktemp)\n    cat > \"$temp_file\" << 'EOF'\n# Feature Verification: test\n\n**Verdict:** VERIFIED\n\n## Summary\nAll good.\nEOF\n\n    # Source extract function\n    source \"$PROJECT_ROOT/ralph-hybrid\"\n\n    run _verify_extract_verdict \"$temp_file\"\n    rm -f \"$temp_file\"\n    [[ \"$output\" == \"VERIFIED\" ]]\n}\n\n@test \"_verify_extract_verdict extracts NEEDS_WORK\" {\n    local temp_file\n    temp_file=$(mktemp)\n    cat > \"$temp_file\" << 'EOF'\n# Feature Verification: test\n\n**Verdict:** NEEDS_WORK\n\n## Summary\nIssues found.\nEOF\n\n    source \"$PROJECT_ROOT/ralph-hybrid\"\n\n    run _verify_extract_verdict \"$temp_file\"\n    rm -f \"$temp_file\"\n    [[ \"$output\" == \"NEEDS_WORK\" ]]\n}\n\n@test \"_verify_extract_verdict extracts BLOCKED\" {\n    local temp_file\n    temp_file=$(mktemp)\n    cat > \"$temp_file\" << 'EOF'\n# Feature Verification: test\n\n**Verdict:** BLOCKED\n\n## Summary\nCritical issues.\nEOF\n\n    source \"$PROJECT_ROOT/ralph-hybrid\"\n\n    run _verify_extract_verdict \"$temp_file\"\n    rm -f \"$temp_file\"\n    [[ \"$output\" == \"BLOCKED\" ]]\n}\n\n@test \"_verify_extract_verdict handles alternate verdict format\" {\n    local temp_file\n    temp_file=$(mktemp)\n    cat > \"$temp_file\" << 'EOF'\n# Feature Verification\n\nVerdict: VERIFIED\n\nAll good.\nEOF\n\n    source \"$PROJECT_ROOT/ralph-hybrid\"\n\n    run _verify_extract_verdict \"$temp_file\"\n    rm -f \"$temp_file\"\n    [[ \"$output\" == \"VERIFIED\" ]]\n}\n\n@test \"_verify_extract_verdict returns empty for missing verdict\" {\n    local temp_file\n    temp_file=$(mktemp)\n    cat > \"$temp_file\" << 'EOF'\n# Feature Verification\n\nNo verdict here.\nEOF\n\n    source \"$PROJECT_ROOT/ralph-hybrid\"\n\n    run _verify_extract_verdict \"$temp_file\"\n    rm -f \"$temp_file\"\n    [[ -z \"$output\" ]]\n}\n\n#==============================================