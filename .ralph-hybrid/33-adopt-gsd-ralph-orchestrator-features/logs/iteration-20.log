ent\" {\n   287→    local result\n   288→    result=$(memory_get_stats \"\")\n   289→    [[ \"$result\" == \"chars:0 tokens:0\" ]]\n   290→}\n   291→\n   292→@test \"memory_fits_budget returns 0 for content within budget\" {\n   293→    local content=\"Short\"  # 5 chars = ~1 token\n   294→    run memory_fits_budget \"$content\" 1000\n   295→    [[ \"$status\" -eq 0 ]]\n   296→}\n   297→\n   298→@test \"memory_fits_budget returns 1 for content exceeding budget\" {\n   299→    local large_content\n   300→    large_content=$(printf 'A%.0s' {1..5000})  # 5000 chars = ~1250 tokens\n   301→    run memory_fits_budget \"$large_content\" 100  # 100 tokens = 400 chars\n   302→    [[ \"$status\" -ne 0 ]]\n   303→}\n   304→\n   305→@test \"memory_fits_budget uses default budget when not specified\" {\n   306→    local content=\"Short\"\n   307→    run memory_fits_budget \"$content\"\n   308→    [[ \"$status\" -eq 0 ]]\n   309→}\n   310→\n   311→#=============================================================================\n   312→# Memory File Creation Tests\n   313→#=============================================================================\n   314→\n   315→@test \"memory_create_template creates file with template structure\" {\n   316→    local memory_file=\"$TEST_TEMP_DIR/new_memories.md\"\n   317→    memory_create_template \"$memory_file\"\n   318→    [[ -f \"$memory_file\" ]]\n   319→    local content\n   320→    content=$(cat \"$memory_file\")\n   321→    [[ \"$content\" == *\"# Memories\"* ]]\n   322→    [[ \"$content\" == *\"## Patterns\"* ]]\n   323→    [[ \"$content\" == *\"## Decisions\"* ]]\n   324→    [[ \"$content\" == *\"## Fixes\"* ]]\n   325→    [[ \"$content\" == *\"## Context\"* ]]\n   326→}\n   327→\n   328→@test \"memory_create_template creates parent directory\" {\n   329→    local memory_file=\"$TEST_TEMP_DIR/nested/dir/memories.md\"\n   330→    memory_create_template \"$memory_file\"\n   331→    [[ -f \"$memory_file\" ]]\n   332→}\n   333→\n   334→@te