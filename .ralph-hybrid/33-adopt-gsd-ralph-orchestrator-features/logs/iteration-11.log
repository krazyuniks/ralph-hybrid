41→spawn_research_agent() {\n   242→    local topic=\"${1:-}\"\n   243→    local output_dir=\"${2:-}\"\n   244→    local template_path=\"${3:-}\"\n   245→\n   246→    # Validate arguments\n   247→    if [[ -z \"$topic\" ]]; then\n   248→        log_error \"spawn_research_agent: Topic is required\"\n   249→        return 1\n   250→    fi\n   251→\n   252→    if [[ -z \"$output_dir\" ]]; then\n   253→        log_error \"spawn_research_agent: Output directory is required\"\n   254→        return 1\n   255→    fi\n   256→\n   257→    # Create output directory if it doesn't exist\n   258→    if [[ ! -d \"$output_dir\" ]]; then\n   259→        mkdir -p \"$output_dir\"\n   260→    fi\n   261→\n   262→    # Check concurrent limit\n   263→    if ! research_can_spawn; then\n   264→        log_warn \"spawn_research_agent: At maximum concurrent agents limit. Waiting...\"\n   265→        wait_for_any_research_agent\n   266→    fi\n   267→\n   268→    # Get configuration\n   269→    local model timeout\n   270→    model=$(research_get_model)\n   271→    timeout=$(research_get_timeout)\n   272→\n   273→    # Sanitize topic for filename\n   274→    local sanitized_topic\n   275→    sanitized_topic=$(_research_sanitize_topic \"$topic\")\n   276→\n   277→    # Generate output filename\n   278→    local output_file\n   279→    output_file=$(printf \"$output_dir/$RALPH_HYBRID_RESEARCH_OUTPUT_PATTERN\" \"$sanitized_topic\")\n   280→\n   281→    # Get the research prompt\n   282→    local prompt\n   283→    prompt=$(_research_get_prompt \"$topic\" \"$template_path\")\n   284→\n   285→    log_info \"Spawning research agent for: $topic\"\n   286→    log_debug \"  Model: $model\"\n   287→    log_debug \"  Output: $output_file\"\n   288→    log_debug \"  Timeout: ${timeout}s\"\n   289→\n   290→    # Spawn Claude in background with --print for non-interactive mode\n   291→    # Use timeout to prevent runaway agents\n   292→    (\n   293→        timeout \"$timeout\" claude --model \"$model\" -